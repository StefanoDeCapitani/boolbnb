        "sass": "^1.15.2",
        "sass-loader": "^8.0.0",
        "vue": "^2.5.17",
        "vue-template-compiler": "^2.6.10"
    },
    "dependencies": {
        "@popperjs/core": "^2.11.2",
        "@tomtom-international/web-sdk-maps": "^6.18.0",
        "@tomtom-international/web-sdk-plugin-searchbox": "^3.1.12",
        "@tomtom-international/web-sdk-services": "^6.18.0"
    }
